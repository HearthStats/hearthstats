var newrow = '<tr><td><%= @deck.nil? ? "null" : link_to(@deck.name, deck_path(@deck)) %></td><td><%= @constructed.oppclass.nil? ? "null" : @constructed.oppclass.name %></td><td class="<%= Match::RESULTS_LIST[@constructed.result_id] %>"><%= Match::RESULTS_LIST[@constructed.result_id] %></td><td><%= !@constructed.coin ? "No" : "" %> Coin</td><td><% if !@constructed.mode.nil? %><% if @constructed.mode.name == "Ranked" && !@constructed.rank.nil? %><%= @constructed.rank.id %> - <%= @constructed.rank.name %><% else %><%= @constructed.mode.name %><% end %><% else %> null <% end %></td><td><%= @constructed.oppname.nil? ? "" : @constructed.oppname %></td><td><%= @constructed.numturns.nil? ? "" : @constructed.numturns %></td><td><%= @constructed.duration.nil? ? "" : seconds_to_short_readable(@constructed.duration) %></td><% if current_user.guest %><td><%= @constructed.created_at.strftime("%b %d, %Y %H:%M") %></td><% else %><td><%= @constructed.created_at.in_time_zone(current_user.profile.time_zone).strftime("%b %d, %Y %H:%M") %></td><% end %><td class="cl-effect-3"><%= link_to(t('shared.edit'), edit_constructed_path(@constructed), class: 'options-button btn default btn-xs purple fa fa-pencil-square-o') %><%= link_to(t('shared.delete'), @constructed, method: :delete, data: { confirm: 'Are you sure?' }, class: 'options-button btn default btn-xs red fa fa-trash-o') %><%= link_to(t('shared.view'), @constructed, class: 'options-button btn default btn-xs blue fa fa-trash-o') %><% if !@constructed.notes.blank? %><%= link_to(t('shared.notes'), @constructed, class: 'options-button btn default btn-xs green fa fa-comment-o') %><% end %></td></tr>' 

toastr.success('Constructed Game Added');
$(newrow).hide().prependTo('.table > tbody:first').fadeIn(999); 